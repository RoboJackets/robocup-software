{
    "problemMatcher": [
        {
            "owner": "cmake",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+): (.+) \\[(.+)\\]",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 5,
                    "code": 6,
                    "severity": 4
                }
            ]
        }
    ]
}
