#ifndef EIGEN_SPARSE_MODULE_H
#define EIGEN_SPARSE_MODULE_H

/** defgroup Sparse_modules Sparse modules
  *
  * Meta-module including all related modules:
  * - SparseCore
  * - OrderingMethods
  * - SparseCholesky
  * - SparseLU
  * - SparseQR
  * - IterativeLinearSolvers
  *
  * \code
  * #include <Eigen/Sparse>
  * \endcode
  */

#include "SparseCore"
