<constants>
  <config_server>
    <use_sim_time>False</use_sim_time>
  </config_server>
  <global_parameter_server>
    <soccer>
      <physics>
        <ball_decay_constant>0.18</ball_decay_constant>
      </physics>
      <robot>
        <chip_angle>40.0</chip_angle>
        <max_acceleration>2.0</max_acceleration>
        <max_chip_speed>4.0</max_chip_speed>
        <max_dribbler_speed>2000.0</max_dribbler_speed>
        <max_kick_speed>7.0</max_kick_speed>
        <max_speed>2.0</max_speed>
        <min_chip_speed>0.5</min_chip_speed>
        <min_kick_speed>1.0</min_kick_speed>
        <min_safe_kick_power>64</min_safe_kick_power>
        <robot_radius>0.09</robot_radius>
      </robot>
    </soccer>
    <use_sim_time>False</use_sim_time>
  </global_parameter_server>
  <control>
    <max_acceleration>1.5</max_acceleration>
    <max_angular_velocity>15.0</max_angular_velocity>
    <max_velocity>2.0</max_velocity>
    <rotation_kd>0.0</rotation_kd>
    <rotation_ki>0.0</rotation_ki>
    <rotation_kp>8.0</rotation_kp>
    <rotation_windup>0</rotation_windup>
    <translation_kd>0.0</translation_kd>
    <translation_ki>0.0</translation_ki>
    <translation_kp>4.0</translation_kp>
    <translation_windup>0</translation_windup>
    <use_sim_time>False</use_sim_time>
  </control>
  <planner>
    <collect>
      <approach_accel_scale>0.7</approach_accel_scale>
      <approach_dist_target>0.04</approach_dist_target>
      <ball_speed_approach_direction_cutoff>0.1</ball_speed_approach_direction_cutoff>
      <control_accel_scale>0.5</control_accel_scale>
      <dist_cutoff_to_approach>1.0</dist_cutoff_to_approach>
      <dist_cutoff_to_control>0.0285</dist_cutoff_to_control>
      <stop_dist_scale>0.5</stop_dist_scale>
      <target_point_lowpass_gain>0.8</target_point_lowpass_gain>
      <touch_delta_speed>0.05</touch_delta_speed>
      <vel_cutoff_to_control>0.3</vel_cutoff_to_control>
      <velocity_control_scale>0.5</velocity_control_scale>
    </collect>
    <constraints>
      <max_rotational_accel>3.0</max_rotational_accel>
      <max_rotational_speed>3.0</max_rotational_speed>
      <max_translational_accel>3.0</max_translational_accel>
      <max_translational_speed>2.0</max_translational_speed>
    </constraints>
    <escape>
      <goal_change_threshold>0.9</goal_change_threshold>
      <step_size>0.1</step_size>
    </escape>
    <pivot>
      <radius_multiplier>1.0</radius_multiplier>
    </pivot>
    <replanner>
      <off_path_threshold>0.1</off_path_threshold>
      <partial_replan_lead_time>0.1</partial_replan_lead_time>
      <pos_change_threshold>0.025</pos_change_threshold>
      <vel_change_threshold>0.025</vel_change_threshold>
    </replanner>
    <rrt>
      <enable_debug_drawing>False</enable_debug_drawing>
      <goal_bias>0.3</goal_bias>
      <max_iterations>250</max_iterations>
      <min_iterations>0</min_iterations>
      <step_size>0.15</step_size>
      <waypoint_bias>0.5</waypoint_bias>
    </rrt>
    <settle>
      <ball_speed_percent_for_dampen>0.1</ball_speed_percent_for_dampen>
      <ball_vel_gain>0.9</ball_vel_gain>
      <intercept_buffer_time>0.3</intercept_buffer_time>
      <max_ball_angle_for_reset>20.0</max_ball_angle_for_reset>
      <max_ball_vel_for_path_reset>2.0</max_ball_vel_for_path_reset>
      <max_bounce_angle>45.0</max_bounce_angle>
      <search_end_dist>6.0</search_end_dist>
      <search_inc_dist>0.2</search_inc_dist>
      <search_start_dist>0.0</search_start_dist>
      <shortcut_dist>0.09</shortcut_dist>
      <target_point_gain>0.5</target_point_gain>
    </settle>
    <timeout>0.1</timeout>
    <use_sim_time>False</use_sim_time>
  </planner>
  <vision_receiver>
    <hz>120.0</hz>
    <port>10060</port>
    <use_sim_time>False</use_sim_time>
    <vision_interface>False</vision_interface>
  </vision_receiver>
  <vision_filter>
    <ball>
      <init_covariance>100.0</init_covariance>
      <observation_noise>2.0</observation_noise>
      <process_noise>0.1</process_noise>
    </ball>
    <camera>
      <max_num_kalman_balls>10</max_num_kalman_balls>
      <max_num_kalman_robots>10</max_num_kalman_robots>
      <mhkf_radius_cutoff>0.5</mhkf_radius_cutoff>
      <use_mhkf>True</use_mhkf>
    </camera>
    <filter>
      <health>
        <dec>1</dec>
        <inc>2</inc>
        <init>2</init>
        <max>20</max>
        <min>1</min>
      </health>
    </filter>
    <kalman_ball>
      <max_time_outside_vision>0.2</max_time_outside_vision>
    </kalman_ball>
    <kalman_robot>
      <max_time_outside_vision>0.5</max_time_outside_vision>
    </kalman_robot>
    <kick>
      <detector>
        <fast_acceleration_trigger>750.0</fast_acceleration_trigger>
        <fast_kick_hist_length>3</fast_kick_hist_length>
        <fast_kick_timeout>1.0</fast_kick_timeout>
        <same_kick_timeout>0.5</same_kick_timeout>
        <slow_any_robot_past_dist>0.16</slow_any_robot_past_dist>
        <slow_kick_hist_length>5</slow_kick_hist_length>
        <slow_kick_timeout>0.5</slow_kick_timeout>
        <slow_max_kick_angle>0.34</slow_max_kick_angle>
        <slow_min_ball_speed>0.6</slow_min_ball_speed>
        <slow_one_robot_within_dist>0.15</slow_one_robot_within_dist>
        <slow_robot_dist_filter_cutoff>3.0</slow_robot_dist_filter_cutoff>
      </detector>
    </kick>
    <max_num_cameras>12</max_num_cameras>
    <publish_hz>120.0</publish_hz>
    <robot>
      <init_covariance>100.0</init_covariance>
      <observation_noise>2.0</observation_noise>
      <orientation_scale>1.0</orientation_scale>
      <process_noise>0.5</process_noise>
    </robot>
    <use_sim_time>False</use_sim_time>
    <vision_filter>
      <bounce>
        <robot_body_angle_dampen>0.0</robot_body_angle_dampen>
        <robot_body_lin_dampen>0.9</robot_body_lin_dampen>
        <robot_mouth_angle_dampen>0.0</robot_mouth_angle_dampen>
        <robot_mouth_lin_dampen>0.3</robot_mouth_lin_dampen>
      </bounce>
    </vision_filter>
    <vision_loop_dt>0.016666666666666666</vision_loop_dt>
    <world_ball>
      <ball_merger_power>1.5</ball_merger_power>
    </world_ball>
    <world_robot>
      <robot_merger_power>1.5</robot_merger_power>
    </world_robot>
  </vision_filter>
  <internal_referee>
    <interface>1.5</interface>
    <team_name>1.5</team_name>
    <use_sim_time>False</use_sim_time>
  </internal_referee>
  <gameplay_node>
    <use_sim_time>False</use_sim_time>
  </gameplay_node>
</constants>
